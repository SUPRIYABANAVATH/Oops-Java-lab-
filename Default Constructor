class Student {
    String name;
    int age;
    Student()  //default
    {
        name = "supriya";
        age = 19;
        System.out.println("Default Constructor called!");
    }
    void display()
    {
        System.out.println("Name: " + name + ", Age: " + age);
    }
}

public class DefaultConstructorExample {
    public static void main(String[] args) {
        Student s1 = new Student();
        s1.display();
    }
}

